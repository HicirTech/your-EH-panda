  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop":true,
    "target": "es6"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules",]
}